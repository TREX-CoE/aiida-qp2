{
    "name": "qp2",
    "author": "TREX-CoE",
    "author_email": "posenitskiy@irsamc.ups-tlse.fr",
    "description": "AiiDA plugin for the Quanum Package 2.0",
    "url": "https://github.com/q-posev/aiida-qp2",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python",
        "Intended Audience :: Science/Research",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0a",
    "entry_points": {
        "aiida.data": [
            "qp2 = qp2.data:QpParameters"
        ],
        "aiida.calculations": [
            "qp2 = qp2.calculations:QpCalculation"
        ],
        "aiida.parsers": [
            "qp2 = qp2.parsers:QpParser"
        ],
        "aiida.cmdline.data": [
            "qp2 = qp2.cli:data_cli"
        ]
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=1.1.0,<2.0.0",
        "sqlalchemy<1.4",
        "six",
        "psycopg2-binary<2.9",
        "voluptuous",
        "pymatgen>=3.3.5"
    ],
    "extras_require": {
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=6.0",
            "pytest-cov",
            "pymatgen"
        ],
        "pre-commit": [
            "pre-commit~=2.2",
            "pylint>=2.5.0,<2.9"
        ],
        "docs": [
            "sphinx",
            "sphinxcontrib-contentui",
            "sphinxcontrib-details-directive; python_version>='3.0'",
            "sphinx-rtd-theme"
        ]
    }
}
